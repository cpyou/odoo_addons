<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
        <record id="view_report_sql_to_excel" model="ir.ui.view">
            <field name="name">sql报表</field>
            <field name="model">sql.to.excel.wizard</field>
            <field name="arch" type="xml">
               <form string="报表测试">
                    <group>
                        <group>
                            <field name="sql_data_id"/>
                        </group>

                    </group>
                 <footer>
                    <button name="report_print" string="打印" type="object" class="oe_highlight"/>
                    or
                    <button string="取消" class="oe_link" special="cancel" />
                 </footer>
                </form>
            </field>
        </record>

        <record id="action_report_sql_to_excel" model="ir.actions.act_window">
            <field name="name">sql报表</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sql.to.excel.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_report_sql_to_excel"/>
            <field name="target">new</field>
        </record>
</data>
</openerp>